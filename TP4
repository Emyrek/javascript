var tab=[10,15,6,14,14,13,19,10,17,9];
var i=0;
var bool="true";

function affTab(tab)
{
	for(i=0;i<10;i++){
	document.getElementById("aaa").innerHTML=document.write(tab[i] + " ");
}


	i=0;
	while(i<10){
		document.getElementById("aaa").innerHTML=document.write(tab[i] + " ");
		i++;
	}

}

function test()
{
	document.getElementById("aaa").innerHTML=document.write("42");
}

affTab(tab);
